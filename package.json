{
  "name": "course-directory",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "react-scripts": "1.0.6"
  },
  "dependencies": {
    "coveralls": "^3.0.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
